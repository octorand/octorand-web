{
  "name": "octorand-web",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:testnet && npm run build:mainnet",
    "build:testnet": "ng build --configuration testnet --output-path=dist/testnet",
    "build:mainnet": "ng build --configuration mainnet --output-path=dist/mainnet",
    "start:testnet": "ng serve -o --port 8810 --configuration preview-testnet",
    "start:mainnet": "ng serve -o --port 8820 --configuration preview-mainnet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@blockshake/defly-connect": "^1.1.6",
    "@perawallet/connect": "^1.3.4",
    "algosdk": "^2.8.0",
    "halfmoon": "^1.1.1",
    "moment": "^2.29.4",
    "ngx-google-analytics": "^14.0.1",
    "rxjs": "~7.5.0",
    "ts-md5": "^1.3.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.0.0",
    "@types/uuid": "^9.0.2",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.9.5"
  }
}
